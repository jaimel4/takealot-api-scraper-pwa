{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:allow-read-text-file",
    "fs:allow-write-text-file",
    "fs:allow-write-file",
    "fs:allow-app-read",
    "fs:allow-app-write",
    "fs:allow-app-write-recursive",
    "dialog:default",
    "dialog:allow-save",
    "http:default",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://api.takealot.com/*"
        },
        {
          "url": "https://www.takealot.com/*"
        }
      ]
    }
  ]
}
